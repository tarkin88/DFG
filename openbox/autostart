xrdb .Xresources &
# Start URxvt daemon
nitrogen --restore &
dunst -conf .config/dunst/dunstrc &
# Remove tearing
compton --config .config/compton/compton.conf &
xfce4-panel &
urxvtd -q -o -f &
mpd ~/.mpd/config &
pulseaudio --start &
(sleep 1s && mpdscribble) &
(sleep 5s && clipit) &
(sleep 5s && volumeicon) &
(sleep 6s && cbatticon) &
(sleep 6s && nm-applet 2>&1 /dev/null &) &
